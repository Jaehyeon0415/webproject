extends ../layout

block content
    .container
        h1.main-title Users
        
        table.table.table-hover
            thead
                tr
                    th Name
                    th Email
                    th Created At
                    th
            tbody
                each user in users
                    tr
                        td
                            a(href=`/users/${user._id}`)= user.name
                            td= user.email
